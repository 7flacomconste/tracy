{
  "name": "tracy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-free-brands": "^5.0.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.8",
    "@fortawesome/react-fontawesome": "0.0.18",
    "react": "^16.7.0",
    "react-code-prettify": "0.0.1",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.1",
    "react-scripts": "^2.1.3",
    "react-syntax-highlight": "^15.3.1",
    "react-table": "^6.8.0",
    "redux": "^4.0.1"
  },
  "scripts": {
    "start":
      "npx web-ext run -s build --no-input true & echo \"$(find public) $(find src)\" | entr sh -c \"npm run build-ui\" ",
    "build-ui": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "compile-ext": "npx web-ext build -s ./public/",
    "deploy-chrome":
      "npx chrome-webstore-upload-cli upload --source web-ext-artifacts/*.zip --extension-id lcgbimfijafcjjijgjoodgpblgmkckhn",
    "deploy-firefox":
      "npx web-ext sign --api-key \"$API_KEY\" --api-secret \"$API_SECRET\" -s build --id {9a38284d-6fc8-460a-b3b8-2378cfd30fe4} -v",
    "deploy":
      "npm install;npm run build;npm run compile-ext;npm run deploy-chrome;npm run deploy-firefox"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "chrome-webstore-upload-cli": "^1.2.0",
    "web-ext": "^3.1.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "webextensions": true,
      "node": true
    }
  },
  "browserslist": [">0.2%", "not dead", "not ie <= 11", "not op_mini all"]
}
